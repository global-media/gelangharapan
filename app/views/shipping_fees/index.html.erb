    <%= render partial: 'shipping_fees/header', locals: {breadcrumbs: []} %>
    
  	<div class="row">
      <div class="col-md-offset-10 col-md-2">
        <%= link_to 'Add', new_admin_store_shipping_fee_url, alt: "Add", class: "btn btn-primary green" %>
      </div>
    </div>
		
    <!-- BEGIN PAGE CONTENT-->
		<div class="row">
			<div class="col-md-12">
				<!-- BEGIN SAMPLE TABLE PORTLET-->
				<div class="portlet">
					<div class="portlet-title">
						<div class="caption">
							<i class="fa fa-truck"></i>Shipping Fees
						</div>
						<div class="tools">
						  <a class="fullscreen" href="javascript:;" data-original-title="" title=""></a>
         			<a href="javascript:;" class="collapse"></a>
						</div>
					</div>
					<div class="portlet-body">
						<div class="table-scrollable">
							<table class="table table-striped table-bordered table-advance table-hover">
							<thead>
							<tr>
								<th class="col-md-4"><i class="fa fa-info"></i> City</th>
								<th class="col-md-2"><i class="fa fa-dollar"></i> Fee</th>
								<th class="col-md-3">&nbsp;</th>
							</tr>
							</thead>
							<tbody>
							  <% @shipping_fees.each do |shipping_fee| %>
							  <tr>
                  <td><%= shipping_fee.city %></td>
                  <td class="value"><%= money_value(shipping_fee.fee) %></td>
                  <td>
                    <%= link_to raw('<i class="icon-pencil"> Edit</i>'), edit_admin_store_shipping_fee_url(id: shipping_fee.id), class: "btn btn-warning btn-xs" %>
                    <%= link_to raw('<i class="icon-trash"> Delete</i>'), admin_store_shipping_fee_url(id: shipping_fee.id), class: "btn btn-danger btn-xs", method: :delete, data: { confirm: "Are you sure you want to delete this shipping_fee?" } %>
                  </td>
  							</tr>
							  <% end %>
							</tbody>
							</table>
						</div>
					</div>
				</div>
				<!-- END SAMPLE TABLE PORTLET-->
			</div>
		</div>
		<!-- END PAGE CONTENT-->
	  