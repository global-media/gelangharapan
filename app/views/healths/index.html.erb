    <%= render partial: 'healths/header', locals: {breadcrumbs: []} %>
      	
  	<div class="row">
      <div class="col-md-offset-10 col-md-2">
        <%= link_to 'Add', new_admin_contents_health_url, alt: "Add", class: "btn btn-primary green" %>
      </div>
    </div>
		
    <!-- BEGIN PAGE CONTENT-->
		<div class="row">
			<div class="col-md-12">
				<!-- BEGIN SAMPLE TABLE PORTLET-->
				<div class="portlet">
					<div class="portlet-title">
						<div class="caption">
							<i class="fa fa-flag"></i>Healths
						</div>
						<div class="tools">
						  <a class="fullscreen" href="javascript:;" data-original-title="" title=""></a>
         			<a href="javascript:;" class="collapse"></a>
						</div>
					</div>
					<div class="portlet-body">
						<div class="table-scrollable">
							<table class="table table-striped table-bordered table-advance table-hover">
							<thead>
							<tr>
								<th class="col-md-2"><i class="fa fa-question"></i> Status</th>
								<th class="col-md-2"><i class="fa fa-info"></i> Title</th>
								<th class="col-md-3"><i class="fa fa-code"></i> Question</th>
								<th class="col-md-3"><i class="fa fa-code"></i> Answer</th>
								<th class="col-md-2">&nbsp;</th>
							</tr>
							</thead>
							<tbody>
							  <% @healths.each do |health| %>
							  <tr>
                  <td><%= health.status %></td>
                  <td><%= health.title %></td>
                  <td><%= health.short_question %></td>
                  <td><%= health.short_answer %></td>
                  <td class="col-md-3">
                    <%= link_to raw('<i class="icon-pencil"> Edit</i>'), edit_admin_contents_health_url(id: health.id), class: "btn btn-warning btn-xs" %>
                    <%= link_to raw('<i class="icon-trash"> Delete</i>'), admin_contents_health_url(id: health.id), class: "btn btn-danger btn-xs", method: :delete, data: { confirm: "Are you sure you want to delete this health?" } %>
                  </td>
  							</tr>
							  <% end %>
							</tbody>
							</table>
						</div>
					</div>
				</div>
				<!-- END SAMPLE TABLE PORTLET-->
			</div>
		</div>
		<!-- END PAGE CONTENT-->
